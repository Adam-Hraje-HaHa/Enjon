		"ParallaxOcclusionMapping": {

			"Type": "Variable",
			"NumberInputs": 1,
			"NumberOutputs": 1,
			"Inputs": {
				"MinSteps": {
					"PrimitiveType": "float",
					"Default": "8.0"
				},
				"MaxSteps": {
					"PrimitiveType": "float",
					"Default": "32.0"
				},
				"HeightRatio": {
					"PrimitiveType": "float",
					"Default": "0.08"
				},
				"HeightMap": {
					"PrimitiveType": "textureObject",
					"Default": "#DEFAULT"	
				},
				"UV": {
					"PrimitiveType": "vec2",
					"Default": "#TEXCOORDS"
				},
				"AlphaCutoff": {
					"PrimitiveType": "vec2",
					"Default": "vec2(0.0, 1.0)"
				}

			},
			"Outputs": {
				"Value": {
					"PrimitiveType": "vec2", 
					"CodeTemplate": "#NODE_NAME"
				}
			},
			"VariableDeclaration": "vec2 #NODE_NAME;",
			"VariableDefinition": [
				"\t// Parallax Occlusion Mapping\n",
				"{\n",
					"\tvec3 viewDir = normalize(#TANGENT_SPACE_VIEW_POS - #TANGENT_SPACE_FRAG_POS);\n",
					"\tfloat numSteps = mix(#INPUT(MaxSteps), #INPUT(MinSteps), abs(dot(vec3(0.0, 0.0, 1.0), viewDir) ) );\n",
					"\tnumSteps = min(numSteps, 512.0);\n\n",

					"\tfloat layerDepth = 1.0 / numSteps;\n",
					"\tfloat currentLayerDepth = 0.0;\n",
					"\tvec2 P = (viewDir.xy / viewDir.z) * #INPUT(HeightRatio);\n",
					"\tvec2 deltaTexCoords = P / numSteps;\n\n",

					"\tvec2 currentTexCoords = #INPUT(UV);\n",
					"\tfloat currentDepthMapValue = texture(#INPUT(HeightMap), currentTexCoords).r;\n\n",

					"\twhile( currentLayerDepth < currentDepthMapValue )\n",
					"\t{\n",
						"\t\tcurrentTexCoords -= deltaTexCoords;\n",
						"\t\tcurrentDepthMapValue = texture(#INPUT(HeightMap), currentTexCoords).r;\n",
						"\t\tcurrentLayerDepth += layerDepth;\n",
					"\t}\n\n",

					"\tvec2 prevTexCoords = currentTexCoords + deltaTexCoords;\n\n",

					"\tfloat afterDepth = currentDepthMapValue - currentLayerDepth;\n",
					"\tfloat beforeDepth = texture(#INPUT(HeightMap), prevTexCoords).r - currentLayerDepth + layerDepth;\n\n",

					"\tfloat weight = afterDepth / (afterDepth - beforeDepth);\n",
					"\tvec2 finalTexCoords = prevTexCoords * weight + currentTexCoords * (1.0 - weight);\n\n",

					"\t#NODE_NAME = finalTexCoords;\n",
				"}\n"
			]
		},

